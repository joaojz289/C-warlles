#include <stdio.h>

int main() {
    float raio, altura, volume;
    const float pi=3.14159;

    printf("Digite o raio da lata (cm): ");
    scanf("%f", &raio);
    
    printf("Informe a altura da lata(cm): ");
    scanf("%f", &altura);

    volume = pi*raio*raio*altura;

    printf("O volume da lata Ã©: %.2f\n", volume);

    return 0;
}
